{
  "name": "@mu-v/cli",
  "version": "0.0.0",
  "private": true,
  "bin": {
    "mu-v": "index.js"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "index.js"
  ],
  "devDependencies": {
    "@mu-v/shared": "workspace:*",
    "typescript": "^4.9.3"
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --no-splitting",
    "dev": "npm run build -- --watch"
  }
}
